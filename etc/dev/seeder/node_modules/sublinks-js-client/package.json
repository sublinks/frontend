{
    "name": "sublinks-js-client",
    "version": "0.0.3",
    "description": "JavaScript/TypeScript client for Sublinks",
    "repository": "https://github.com/sublinks/js-client",
    "license": "AGPL-3.0",
    "author": "Admiral Patrick <asimons04@gmail.com>",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "sourceRoot": "./src",
    "files": [
        "dist/"
      ],
    "scripts": {
        "build": "tsc",
        "docs": "typedoc src/index.ts"
    },
    "publishConfig": {
        "ignore": [
            "docs/",
            "src/",
            "node_modules/",
            "tsconfig.json",
            "README.md",
            ".gitignore"
        ]
    },
    "private": false,
    "type": "commonjs",
    "dependencies": {
        "cross-fetch": "^3.1.5",
        "lemmy-js-client": "^0.19.0-rc.19"
    },
    "devDependencies": {
        "@types/node": "^20.10.3",
        "typedoc": "^0.25.4",
        "typescript": "^5.3.2"
    }
}
